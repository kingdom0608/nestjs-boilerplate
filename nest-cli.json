{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-boilerplate/src",
  "monorepo": true,
  "root": "apps/nestjs-boilerplate",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nestjs-boilerplate/tsconfig.app.json"
  },
  "projects": {
    "api-user": {
      "type": "application",
      "root": "apps/api-user",
      "entryFile": "main",
      "sourceRoot": "apps/api-user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-user/tsconfig.app.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "product": {
      "type": "library",
      "root": "libs/product",
      "entryFile": "index",
      "sourceRoot": "libs/product/src",
      "compilerOptions": {
        "tsConfigPath": "libs/product/tsconfig.lib.json"
      }
    }
  }
}
